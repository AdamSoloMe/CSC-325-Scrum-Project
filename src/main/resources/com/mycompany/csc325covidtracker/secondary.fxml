                  <RadioButton mnemonicParsing="false" text="RadioButton" toggleGroup="$ethn" />
                  <RadioButton mnemonicParsing="false" selected="true" text="Rather Not Say" toggleGroup="$ethn">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </RadioButton>
               </children>
            </VBox>
            <VBox layoutX="278.0" layoutY="340.0" prefHeight="63.0" prefWidth="241.0">
               <children>
                  <RadioButton mnemonicParsing="false" text="Yes, I am Vaccinated">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </RadioButton>
                  <RadioButton mnemonicParsing="false" selected="true" text="No, I am not Vaccinated">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <toggleGroup>
                        <ToggleGroup fx:id="VaccStatus" />
                     </toggleGroup>
